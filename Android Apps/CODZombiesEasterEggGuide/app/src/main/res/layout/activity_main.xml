<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.t00229613.codzombieseastereggguide.MainActivity"
    android:orientation="vertical"
    android:background="@drawable/m">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.example.t00229613.codzombieseastereggguide.Title_Fragment"
        android:id="@+id/fragment"
        android:layout_weight="2" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.example.t00229613.codzombieseastereggguide.List_Fragment"
        android:id="@+id/fragment2"
        android:layout_gravity="center_vertical"
        android:layout_weight="1" />
</LinearLayout>
